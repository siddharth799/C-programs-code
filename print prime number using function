check wheather a given number is prime or not

#include<stdio.h>
int main(){
    int n;
    printf("enter number");
    scanf("%d",&n);
    if(isprime(n))
        printf("%d is prime",n);
    else
        printf("%d is not prime",n);
    
}
int isprime(int n){
    int i,count=0;
    for(i=2;i<n;i++){
        if(n%i==0)
            return 0;
    }
}
--------------------------------------------------------------------------------------------------------------------------------
print prime number from 1 to n using function

#include<stdio.h>
int main(){
    int n,k;
    printf("enter number");
    scanf("%d",&n);
    for(k=1;k<=n;k++){
    if(isprime(k))
        printf("%d  ",k);
    }
}
int isprime(int k){
    int i,count=0;
    for(i=2;i<k;i++){
        if(k%i==0)
            return 0;
    }
}
